	#Кодировка#
AddDefaultCharset utf-8
	#Директория#
DirectoryIndex index.php
Options -Indexes
php_value date.timezone "Europe/Moscow"

RewriteEngine On

	#Разные реврайты#
RewriteRule ^login?$ modules/users/src/login.php [L]
RewriteRule ^reg?$ modules/users/src/reg.php [L]
RewriteRule ^online?$ modules/users/src/online.php [L]
RewriteRule ^exit?$ modules/users/src/exit.php [L]
