	#Реврайты для blogs#
RewriteRule ^blogs/?$ modules/blogs/src/index.php [L]
RewriteRule ^blogs/add/?$ modules/blogs/src/add.php [L]
RewriteRule ^blogs/view([0-9]*)/?$ modules/blogs/src/view.php?id=$1 [L,QSA]
RewriteRule ^blogs/ublogs([0-9]*)/?$ modules/blogs/src/ublogs.php?id=$1 [L,QSA]
RewriteRule ^blogs/comment([0-9]*)?$ modules/blogs/src/comment.php?id=$1 [L,QSA]
RewriteRule ^blogs/comment([0-9]*)/otv([0-9]*)/?$ modules/blogs/src/comment.php?id=$1&otv=$2 [L,QSA]
RewriteRule ^blogs/comment([0-9]*)/del([0-9]*)/?$ modules/blogs/src/comment.php?id=$1&del&cid=$2 [L,QSA]

