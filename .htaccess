RewriteRule ^lib/comment([0-9]*)/?$ modules/lib/src/comment.php?id=$1 [L,QSA]
RewriteRule ^lib/?$ modules/lib/src/index.php [L]
RewriteRule ^lib/comment([0-9]*)/otv([0-9]*)/?$ modules/lib/src/comment.php?id=$1&otv=$2 [L,QSA]
