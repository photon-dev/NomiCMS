RewriteRule ^news/?$ modules/news/index.php [L]
RewriteRule ^news/comment([0-9]*)/?$ modules/news/src/comment.php?id=$1 [L,QSA]
RewriteRule ^news/comment([0-9]*)/otv([0-9]*)/?$ modules/news/src/comment.php?id=$1&otv=$2 [L,QSA]
RewriteRule ^news/comment([0-9]*)/del([0-9]*)/?$ modules/news/src/comment.php?id=$1&del&cid=$2 [L,QSA]

